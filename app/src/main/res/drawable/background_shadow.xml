<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#CB969696" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item
        android:right="1dp"
        android:left="1dp"
        android:bottom="3dp">
        <shape android:shape="rectangle">
            <solid android:color="#D3ADE0FC" />
            <corners android:radius="5dp" />
        </shape>
    </item>
</layer-list>